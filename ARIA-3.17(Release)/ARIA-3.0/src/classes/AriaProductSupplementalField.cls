	public static map<string,string> getPlanInstanceFieldsMap(){
		map<string,string> mapToReturn = new map<string,string>();
		Map<String,Aria_Product_Supplemental_Field__c> fieldsMap = getPlanAllSuppMap();
		for(string fieldApiName:fieldsMap.keySet()){
			mapToReturn.put(fieldApiName,fieldsMap.get(fieldApiName).Name);
		}
		return mapToReturn;
	}
	
	public static Map<String,Aria_Product_Supplemental_Field__c> getPlanInstanceSuppMap(){
		return supplementFieldMap('where Is_Active__c=true and Can_Override__c = true');
	}
	
	public static Map<String,Aria_Product_Supplemental_Field__c> getPlanAllSuppMap(){
		return supplementFieldMap('where Is_Active__c=true');
	}
	
	
	
	
	static Map<String,Aria_Product_Supplemental_Field__c> suppFldMap;
	public static Map<String,Aria_Product_Supplemental_Field__c>  getSupplementFieldMap(String condition){
		String query = 'select ID, product_field_no__c,Field_Value__c, Field_Name__c,max_no_sel__c,min_no_sel__c,Type__c';
		query += ',Name, allowed_values__c,Can_Override__c,AccountPlanField_ApiName__c,Display_Type__c,ClientPlanField_ApiName__c from Aria_Product_Supplemental_Field__c';
		if(!String.isEmpty(condition)){
			query += ' ' + condition; 
		}
		System.debug('====query======'+query);
		if(suppFldMap != null){
			return suppFldMap;
		}
		List<Aria_Product_Supplemental_Field__c> suppFieldObj = Database.query(query);
		suppFldMap = new Map<String,Aria_Product_Supplemental_Field__c>();
		Map<String, Schema.SObjectField> AccountPlanFieldsMap = getFieldsmap(new string[]{AriaConstants.PREFIX+'Account_Plan__c'});
		System.debug('=====AccountPlanFieldsMap==='+AccountPlanFieldsMap.keyset());
		for(Aria_Product_Supplemental_Field__c suppField : suppFieldObj){
			if(String.isNotBlank(suppField.AccountPlanField_ApiName__c)){
				System.debug('====fieldExists========='+suppField.AccountPlanField_ApiName__c+'  ->'+fieldExists(AccountPlanFieldsMap,suppField.AccountPlanField_ApiName__c));
				if(fieldExists(AccountPlanFieldsMap,suppField.AccountPlanField_ApiName__c)){
					suppFldMap.put(suppField.AccountPlanField_ApiName__c,suppField);
				}
			}
		}
		return suppFldMap;
	}
	
	public static Map<String,Aria_Product_Supplemental_Field__c>  supplementFieldMap(String condition){
		String query = 'select ID, product_field_no__c,Field_Value__c, Field_Name__c,max_no_sel__c,min_no_sel__c,Type__c';
		query += ',Name, allowed_values__c,Can_Override__c,AccountPlanField_ApiName__c,Display_Type__c,ClientPlanField_ApiName__c from Aria_Product_Supplemental_Field__c';
		if(!String.isEmpty(condition)){
			query += ' ' + condition; 
		}
		System.debug('====query======'+query);
		List<Aria_Product_Supplemental_Field__c> suppFieldObj = Database.query(query);
		Map<String,Aria_Product_Supplemental_Field__c> suppFieldMap = new Map<String,Aria_Product_Supplemental_Field__c>();
		Map <String, Schema.SObjectField> AccountPlanFieldsMap = getFieldsmap(new string[]{AriaConstants.PREFIX+'Account_Plan__c'});
		Map <String, Schema.SObjectField> ClientPlanFieldsMap = getFieldsmap(new string[]{AriaConstants.PREFIX+'Client_Plan__c'});
		System.debug('=====AccountPlanFieldsMap==='+AccountPlanFieldsMap.keyset());
		for(Aria_Product_Supplemental_Field__c suppField : suppFieldObj){
			if(String.isNotBlank(suppField.AccountPlanField_ApiName__c)){
				if(suppField.Can_Override__c){
					if(fieldExists(AccountPlanFieldsMap,suppField.AccountPlanField_ApiName__c)){
						suppFieldMap.put(suppField.AccountPlanField_ApiName__c,suppField);
					}
				}
			}
			if(String.isNotBlank(suppField.ClientPlanField_ApiName__c)){
				if(!suppField.Can_Override__c){
					if(fieldExists(ClientPlanFieldsMap,suppField.ClientPlanField_ApiName__c)){
						suppFieldMap.put(suppField.ClientPlanField_ApiName__c,suppField);
					}
				}
			}
		}
		
		System.debug('====suppFieldMap===='+suppFieldMap.size());
		return suppFieldMap;
	}
	public static Map<String,String> setProductParams(Map<String,String> params,Account_Plan__c currentAccountPlan,boolean isUpdateCall){
		Map<String,Aria_Product_Supplemental_Field__c> Aria_Product_Supplemental_FieldMap = new Map<String,Aria_Product_Supplemental_Field__c>();
		String suppFieldName  = '';
        String suppFieldValue = '';
        String suppFieldAssignmentDirective = '';
        String whereclause = 'where Is_Active__c = true and Can_Override__c=true';
        Aria_Product_Supplemental_FieldMap = supplementFieldMap(whereclause);
        if(!Aria_Product_Supplemental_FieldMap.isEmpty()){
            for(String prodField : Aria_Product_Supplemental_FieldMap.keySet()){
            	if(Aria_Product_Supplemental_FieldMap.get(prodField).allowed_values__c != null && currentAccountPlan.get(prodField) == null){
	                
            	} else{
            		if(String.isNotBlank(prodField) && String.isNotBlank(String.ValueOf(currentAccountPlan.get(prodField)))){
            			if(String.ValueOf(currentAccountPlan.get(prodField)).contains(';')){
	            				String prodFieldValue = String.ValueOf(currentAccountPlan.get(prodField));
	            				List<String> val = prodFieldValue.split(';');
	            				for(integer i = 0 ; i < val.size();i++){
	            					suppFieldName += Aria_Product_Supplemental_FieldMap.get(prodField).Field_Name__c == null ? '' + '|' : Aria_Product_Supplemental_FieldMap.get(prodField).Field_Name__c +'|';
		                			suppFieldValue += val[i] == null ? '' + '|' : val[i] +'|';
		                			if(isUpdateCall){
		                				suppFieldAssignmentDirective += '2|';	
		                			}
	            				}
            			}else{
            				suppFieldName += Aria_Product_Supplemental_FieldMap.get(prodField).Field_Name__c == null ? '' + '|' : Aria_Product_Supplemental_FieldMap.get(prodField).Field_Name__c +'|';
            				if(Aria_Product_Supplemental_FieldMap.get(prodField).Display_Type__c == 'Date'){
            					suppFieldValue += currentAccountPlan.get(prodField) == null ? '' + '|' : getDateValue(Date.valueOf(currentAccountPlan.get(prodField))) +'|';	
            				}else{
		                		suppFieldValue += currentAccountPlan.get(prodField) == null ? '' + '|' : currentAccountPlan.get(prodField) +'|';
            				}
		                	if(isUpdateCall){
	            				suppFieldAssignmentDirective += '2|';	 
	            			}
            			}
            		}
            	}
            }
        }
        
	        if(suppFieldName != ''){
	            suppFieldName = suppFieldName.substring(0,suppFieldName.lastIndexOf('|'));
	        }
	        if(suppFieldValue != ''){
	            suppFieldValue = suppFieldValue.substring(0,suppFieldValue.lastIndexOf('|'));
	        }
	        if(suppFieldAssignmentDirective != '' && isUpdateCall){
	           suppFieldAssignmentDirective = suppFieldAssignmentDirective.substring(0,suppFieldAssignmentDirective.lastIndexOf('|'));
	        }
	        if(suppFieldName != ''){
	            params.put('plan_instance_field_name',suppFieldName);
	        }
	        if(suppFieldValue != ''){
	            params.put('plan_instance_field_value',suppFieldValue);
	        }
			if(suppFieldAssignmentDirective != '' && isUpdateCall){
	           params.put('plan_instance_field_directive',suppFieldAssignmentDirective );
	       }
        return params;
	}
	
	public static string getDateValue(Date dt){
    	if(dt == null){
    		return '';
    	}
    	//12/3/2013 2013-12-3
    	//return ''+dt.Month()+'/'+dt.Day()+'/'+dt.year();
    	return ''+dt.year()+'-'+dt.Month()+'-'+dt.Day();
    }
