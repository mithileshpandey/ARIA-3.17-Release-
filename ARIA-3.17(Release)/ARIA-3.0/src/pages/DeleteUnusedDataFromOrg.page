<!-- This file should not be added in the pacakge. -->
<apex:page controller="DeleteUnusedCustomFields">
<style>
input[type=checkbox] {
   width: 19px;
   height: 19px;
   cursor: pointer;
   vertical-align: middle;
}
</style> 
<apex:form >
    <apex:pageBlock title="Delete Unused Data/Fields From Org.">
        <apex:pageBlockSection title="Select checkbox to perform an operation." columns="1">
            <apex:pageBlockSectionItem > 
                <apex:selectcheckboxes value="{!selectedCheckboxValues}" layout="pageDirection">
                    <apex:selectOption itemLabel="Delete Custom Supp Fields Generated by metadata Api on 'Account Plan' Object" itemValue="DelAPFields"/>
                    <apex:selectOption itemLabel="Delete Custom Supp Fields Generated by metadata Api on 'Client Plan' Object" itemValue="DelCPFields"/>    
                    <apex:selectOption itemLabel="Delete all records from 'Aria Product Supplemental Field' Object" itemValue="DelProductSuppRecords"/>
                    <apex:selectOption itemLabel="Reset Aria Account Functional Group picklist value to dummy" itemValue="resetAccountFunctionGroupPickListValue"/>  
                </apex:selectcheckboxes>     
            </apex:pageBlockSectionItem>
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
            <apex:commandButton action="{!actionDelete}" value="Delete"/>
        </apex:pageBlockButtons>
    </apex:pageBlock>
</apex:form>
</apex:page>